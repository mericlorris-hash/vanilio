<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_move_form_inherit_shopify_ept" model="ir.ui.view">
        <field name="name">stock.move.form.inherit.shopify.ept</field>
        <field name="model">stock.move</field>
        <field name="inherit_id" ref="stock.view_move_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='origin']" position="after">
                <field name="carrier_id" readonly="1" options="{'no_open': True, 'no_create': True}"/>
                <field name="tracking_reference" readonly="1"/>
            </xpath>
        </field>
    </record>
</odoo>